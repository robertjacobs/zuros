launch>
	<!-- Run the map server with the map -->
	<node name="map_server" pkg="map_server" type="map_server" args="$(find
	zuros_navigation)/environments/robot_room/map.yaml"/>
	
	<!-- Launch move_base and load all navigation parameters -->
	<include file="$(find zuros_navigation)/launch/move_base.launch" />

	<!-- Run a static transform between /odom and /map -->
	<node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster"
	args="0 0 0 0 0 0 /odom /map 100" />
</launch>

